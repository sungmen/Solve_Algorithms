#include <bits/stdc++.h>
using namespace std;
int main() {
    //freopen("input.in", "r", stdin);
    ios_base::sync_with_stdio(false),cin.tie(nullptr),cout.tie(nullptr);
    int t; cin >> t;
    while (t--) {
        int n; cin >> n;
        long long ans = 0;
        for (int i = 0; i < n; ++i) {
            int p; cin >> p;
            ans += p;
        }
        cout << ((ans%n) == 0?0:1) << '\n';
    }
    return 0;
}